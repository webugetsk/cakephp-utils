{
  "name": "webugetsk/cakephp-utils",
  "description": "Plugin for CakePHP 4.x with useful components and behaviors",
  "type": "cakephp-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "Bob Mulder",
      "email": "bobmulder@outlook.com"
    },
    {
      "name": "Alim Imanbakiev",
      "email": "info@webuget.sk"
    }
  ],
  "require": {
    "php": ">=7.4",
    "cakephp/cakephp": "^4.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "Utils\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Utils\\Test\\": "tests",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests",
      "App\\": "tests/App"
    }
  }
}
